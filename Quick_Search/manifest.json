{
    "manifest_version": 2,
    "name" : "Pocket Wiki",
    "version" : "1.0",
    "description": "Search any word with one click",
    "icons":{
        "16" : "image/icon16.png",
        "48" : "image/icon48.png",
        "128" : "image/icon128.png"
    },
    "background":{
        "scripts" : ["eventpage.js"],
        "persistent" : false
    },
    "permissions":[
        "tabs",
        "contextMenus"
    ]
}